<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#7CB342">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/logo_name"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:src="@drawable/setting"
        tools:layout_editor_absoluteX="367dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/transparent"
        android:src="@drawable/tune"
        tools:layout_editor_absoluteX="367dp"
        tools:layout_editor_absoluteY="59dp" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rectangle_circle_layout_background_gray"
        android:layout_marginTop="100dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="306dp"
            android:layout_height="340dp"
            android:background="@drawable/rectangle_circle_layout"
            android:backgroundTint="#ffffff"
            tools:layout_editor_absoluteX="52dp"
            tools:layout_editor_absoluteY="16dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="월별 배출한 음식물 쓰레기 양"
                android:textSize="22dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="21dp"
                tools:layout_editor_absoluteY="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="20L"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="83dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10L"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="163dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5L"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="27dp"
                tools:layout_editor_absoluteY="243dp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#b1b1b1"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="30dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="95.5dp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#b1b1b1"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="30dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="175dp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#b1b1b1"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="30dp"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="256dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="95dp"
                tools:layout_editor_absoluteY="269dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="133.5dp"
                tools:layout_editor_absoluteY="269dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="3"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="172dp"
                tools:layout_editor_absoluteY="269dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="210.5dp"
                tools:layout_editor_absoluteY="269dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5"
                android:textColor="#797979"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="249dp"
                tools:layout_editor_absoluteY="269dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="306dp"
            android:layout_height="146dp"
            android:background="@drawable/rectangle_circle_layout"
            android:backgroundTint="#ffffff"
            tools:layout_editor_absoluteX="52dp"
            tools:layout_editor_absoluteY="372dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="이번달에 배출한 음식물 쓰레기 양"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="21dp"
                tools:layout_editor_absoluteY="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="지난달보다 10L 덜 버렸어요"
                android:textSize="19dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="43dp"
                tools:layout_editor_absoluteY="102dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="android:color/transparent"
            android:src="@drawable/trash_minus"
            tools:layout_editor_absoluteX="96dp"
            tools:layout_editor_absoluteY="528dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="android:color/transparent"
            android:src="@drawable/trash_plus"
            tools:layout_editor_absoluteX="247dp"
            tools:layout_editor_absoluteY="528dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:id="@android:id/tabcontent"
        android:layout_height="0dp"
        android:layout_weight="1">

    </FrameLayout>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/store_fragment_tablayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="680dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.tabs.TabItem
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:icon="@drawable/home" />

        <com.google.android.material.tabs.TabItem
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:icon="@drawable/recipe" />

        <com.google.android.material.tabs.TabItem
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:icon="@drawable/trash" />

    </com.google.android.material.tabs.TabLayout>
</androidx.constraintlayout.widget.ConstraintLayout>